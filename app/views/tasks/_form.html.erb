<%= form_for(@task) do |f| %>
  <div class="form-group">
    <%= f.label :tittle %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :details %>
    <%= f.text_field :details, class: 'form-control' %>
  </div>

  <div class="check">
    <%= f.check_box :completed %>
    <%= f.label :completed %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
